{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm run clean && npm run build && node ./dist/server.js",
    "dev": "./node_modules/.bin/nodemon --config ./nodemon.json",
    "debug": "npm run clean && npm run watch && node-debug ./server.js",
    "clean": "./node_modules/.bin/rimraf dist",
    "build": "./node_modules/.bin/tsc",
    "watch": "./node_modules/.bin/tsc --watch",
    "lint": "./node_modules/.bin/tslint ./src/**/*.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.18.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "jsonfile": "^6.0.1",
    "pify": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "steno": "^0.5.0",
    "type-graphql": "^1.0.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/.*\\.test.ts$"
  }
}
